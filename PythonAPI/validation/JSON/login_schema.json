{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "required": [
    "username",
    "password"
  ],
  "properties": {
    "username": {
      "title": "Username",
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]{4,16}$",
      "maxLength": 50,
      "minLength": 2
    },
    "password": {
      "title": "Password",
      "type": "string",
      "pattern": "^.*$",
      "minLength": 7
    }
  }
}
